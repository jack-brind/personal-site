{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/kpis", "destination": "/kpis.html" },
    { "source": "/knowledgebase", "destination": "/knowledgebase.html" },
    {
      "source": "/compliance-overrides",
      "destination": "/compliance-overrides.html"
    },
    {
      "source": "/src/cutting-unnecessary-complexity",
      "destination": "/src/cutting-unnecessary-complexity.html"
    }
  ],
  "redirects": [
    { "source": "/kpis.html", "destination": "/kpis", "permanent": true },
    {
      "source": "/knowledgebase.html",
      "destination": "/knowledgebase",
      "permanent": true
    },
    {
      "source": "/compliance-overrides.html",
      "destination": "/compliance-overrides",
      "permanent": true
    }
  ]
}
